{
  "name": "rs-news",
  "version": "1.0.0",
  "description": "RS News - Mail and Parcel Service News for Family-Owned Stores",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "seed": "node scripts/seedArticles.js",
    "wrangler": "wrangler",
    "wrangler:dev": "wrangler dev",
    "wrangler:deploy": "wrangler deploy",
    "wrangler:deploy:staging": "wrangler deploy --env staging",
    "wrangler:deploy:production": "wrangler deploy --env production",
    "wrangler:tail": "wrangler tail",
    "wrangler:login": "wrangler login",
    "db:create": "wrangler d1 create rsnews-db1",
    "db:execute": "wrangler d1 execute rsnews-db1",
    "db:schema": "wrangler d1 execute rsnews-db1 --file schema.sql",
    "db:backup": "wrangler d1 export rsnews-db1 > backup-$(date +%Y%m%d-%H%M%S).sql",
    "db:list": "wrangler d1 list",
    "lint": "eslint . --ext .js --fix",
    "test": "echo \"No tests configured yet\""
  },
  "keywords": [
    "news",
    "mail",
    "parcel",
    "shipping"
  ],
  "author": "RS",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "connect-mongo": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^8.1.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "wrangler": "^4.60.0"
  }
}
